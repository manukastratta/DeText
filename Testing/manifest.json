{
    "manifest_version": 2,
    "name": "Testing extension",
    "version": "1.0",
    "description": "Chrome browser extension for testing purposes",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ],
    "permissions": [
        "storage" ,
        "tabs",
        "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": "my_logo.png"
    }
}
